  <select id="getOrderItemByUidOrMobile" parameterType="map" resultType="com.trj.jk.web.dto.OrderItemDTO">
    select a.id as id,a.uid as uid,a.loan_limit_id as limitId,a.order_no as orderNo,c.goods_name as goodsName,b.sale_num as saleNum,a.amount as amount,a.`status` as `status`,e.current_period-1 as currentPeriod,d.term as term
    from jk_order a
    left join jk_order_item b on a.id=b.order_id
    LEFT JOIN jk_goods c on b.goods_id=c.id
    LEFT JOIN jk_loan_limit d on a.loan_limit_id=d.id and d.type=1
    LEFT JOIN jk_loan_repay_general e on a.loan_limit_id=e.loan_limit_id
    LEFT JOIN jk_user_basic f on a.uid=f.id
    where a.status!=0
    <if test="uid != null">
      and a.uid=#{uid,jdbcType=INTEGER}
    </if>
    <if test="mobile != null">
      and f.mobile=#{mobile,jdbcType=VARCHAR}
    </if>
    <if test="uid == null and mobile==null">
      and a.uid=0
    </if>

  </select>
